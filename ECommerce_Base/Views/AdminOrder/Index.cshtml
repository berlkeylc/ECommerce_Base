
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="mx-auto" style="width:75%">
    <br />
    <h2>Orders Table</h2>
    <br />

    <div class="d-flex justify-content-end">
        <button type="button" class="btn btn-success UserSaveModal" data-bs-toggle="modal" data-bs-target="#exampleModal4" id="UserSaveModal" name="AddCategory">
            Add New Order
        </button>
    </div>

    <br />
    <table class="table table-striped" id="OrdersTable">
        <thead>
            <tr>
                <th>#</th>
                <th>User ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date</th>
                <th>Required Date</th>
                <th>Shipped Date</th>
                <th>Freight</th>
                <th>Is Delivered</th>
                <th>Delete</th>
                <th>Update</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

